{
	// "$schema": "./waybar.schema.json",

	"include": ["$HOME/.config/waybar/modules.jsonc"],

	"layer": "top",
	"spacing": 1,

	"modules-left": ["custom/menu", "wlr/workspaces", "wlr/taskbar"],

	"wlr/taskbar": {
		"format": "{}"
	},
	"wlr/workspaces": {
		"format": "{icon}",
		"format-icons": {
			"1": "",
			"2": "",
			"3": "",
			"4": "",
			"5": "",
		},
		"sort-by-number": true
	}
}
